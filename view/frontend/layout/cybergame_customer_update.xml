<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Update Room Information</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magenest\Cybergame\Block\Customer\Update" name="cybergame_customer_update" template="Magenest_Cybergame::customer/update.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>

